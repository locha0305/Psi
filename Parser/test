class AddTri {
    # class example

    # method for adding three numbers

    meth add : give(recv N{}; recv P{}; recv K{}) {
        var Res{0}

        Res = N + P + K

        return{Res}
    }

    var p{3 + 4}
    var q{p + 1}
}

var p{3 / 2 + 5}



meth lambda:give(recv x{}){return{2 * x}}

